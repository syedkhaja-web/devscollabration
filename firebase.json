{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "target": "apphosting",
    "source": ".",
    "rewrites": [
      {
        "source": "**",
        "apphosting": "devs-tec-collab"
      }
    ]
  },
  "apphosting": {
    "source": ".",
    "project": "devs-tec-collab",
    "location": "us-central1"
  }
}
